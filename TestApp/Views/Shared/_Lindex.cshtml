    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        .body-wrapp {
            min-width: 94vw;
            background-color:white;
        }

            .body-wrapp .sidemenu {
                font-size: 22px;
                padding: 15px;
                width: 18%;
            }

        .sidemenu > div > p {
            text-decoration: none;
            
        }
        
        .body-wrapp .renderWrapp {
            width: 82%;
        }

        .a {
            text-decoration: none;
        }

        .link-side {
            color: white;
            text-decoration: none;
            margin-top: 9px;
        }

        .font-size-title {
            font-size: 33px;
            padding-left: 30px;
        }

        .top-header {
            padding-left: 0;
            padding-right: 0;
            min-width: 100vw;
        }

        .login-part {
            margin-right: 30px;
        }
    </style>
